{
  "info": {
    "name": "MERN Agent Management API",
    "description": "Complete API collection for MERN Stack Agent Management System",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "variable": [
    {
      "key": "baseUrl",
      "value": "http://localhost:5001/api",
      "type": "string"
    },
    {
      "key": "authToken",
      "value": "",
      "type": "string"
    }
  ],
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Admin Login",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@example.com\",\n  \"password\": \"admin123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["auth", "login"]
            }
          },
          "response": [
            {
              "name": "Successful Login",
              "originalRequest": {
                "method": "POST",
                "header": [],
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"email\": \"admin@example.com\",\n  \"password\": \"admin123\"\n}"
                },
                "url": {
                  "raw": "{{baseUrl}}/auth/login",
                  "host": ["{{baseUrl}}"],
                  "path": ["auth", "login"]
                }
              },
              "status": "OK",
              "code": 200,
              "body": "{\n  \"success\": true,\n  \"message\": \"Login successful\",\n  \"data\": {\n    \"token\": \"eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9...\",\n    \"user\": {\n      \"id\": 1,\n      \"email\": \"admin@example.com\",\n      \"name\": \"Admin User\",\n      \"role\": \"ADMIN\"\n    }\n  }\n}"
            }
          ],
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('authToken', response.data.token);",
                  "    pm.test('Login successful', () => {",
                  "        pm.expect(response.success).to.be.true;",
                  "        pm.expect(response.data.token).to.exist;",
                  "    });",
                  "}"
                ]
              }
            }
          ]
        }
      ]
    },
    {
      "name": "Agents",
      "item": [
        {
          "name": "Get All Agents",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/agents",
              "host": ["{{baseUrl}}"],
              "path": ["agents"]
            }
          },
          "response": [
            {
              "name": "Success Response",
              "originalRequest": {
                "method": "GET",
                "header": [
                  {
                    "key": "Authorization",
                    "value": "Bearer {{authToken}}"
                  }
                ],
                "url": {
                  "raw": "{{baseUrl}}/agents",
                  "host": ["{{baseUrl}}"],
                  "path": ["agents"]
                }
              },
              "status": "OK",
              "code": 200,
              "body": "{\n  \"success\": true,\n  \"data\": [\n    {\n      \"id\": 1,\n      \"name\": \"John Doe\",\n      \"email\": \"john@example.com\",\n      \"phone\": \"+1234567890\",\n      \"createdAt\": \"2024-01-01T00:00:00.000Z\"\n    }\n  ]\n}"
            }
          ]
        },
        {
          "name": "Create Agent",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              },
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"phone\": \"+1234567890\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/agents",
              "host": ["{{baseUrl}}"],
              "path": ["agents"]
            }
          },
          "response": [
            {
              "name": "Agent Created",
              "originalRequest": {
                "method": "POST",
                "header": [
                  {
                    "key": "Authorization",
                    "value": "Bearer {{authToken}}"
                  }
                ],
                "body": {
                  "mode": "raw",
                  "raw": "{\n  \"name\": \"John Doe\",\n  \"email\": \"john@example.com\",\n  \"phone\": \"+1234567890\",\n  \"password\": \"password123\"\n}"
                },
                "url": {
                  "raw": "{{baseUrl}}/agents",
                  "host": ["{{baseUrl}}"],
                  "path": ["agents"]
                }
              },
              "status": "Created",
              "code": 201,
              "body": "{\n  \"success\": true,\n  \"message\": \"Agent created successfully\",\n  \"data\": {\n    \"id\": 1,\n    \"name\": \"John Doe\",\n    \"email\": \"john@example.com\",\n    \"phone\": \"+1234567890\"\n  }\n}"
            }
          ]
        }
      ]
    },
    {
      "name": "Leads",
      "item": [
        {
          "name": "Upload CSV File",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": "test_leads.csv"
                }
              ]
            },
            "url": {
              "raw": "{{baseUrl}}/leads/upload",
              "host": ["{{baseUrl}}"],
              "path": ["leads", "upload"]
            }
          },
          "response": [
            {
              "name": "File Uploaded Successfully",
              "originalRequest": {
                "method": "POST",
                "header": [
                  {
                    "key": "Authorization",
                    "value": "Bearer {{authToken}}"
                  }
                ],
                "body": {
                  "mode": "formdata",
                  "formdata": [
                    {
                      "key": "file",
                      "type": "file",
                      "src": "test_leads.csv"
                    }
                  ]
                },
                "url": {
                  "raw": "{{baseUrl}}/leads/upload",
                  "host": ["{{baseUrl}}"],
                  "path": ["leads", "upload"]
                }
              },
              "status": "OK",
              "code": 200,
              "body": "{\n  \"success\": true,\n  \"message\": \"File uploaded and distributed successfully\",\n  \"data\": {\n    \"totalRecords\": 10,\n    \"distributedTo\": 5,\n    \"distribution\": [\n      {\n        \"agentId\": 1,\n        \"agentName\": \"Agent 1\",\n        \"assignedCount\": 2\n      }\n    ]\n  }\n}"
            }
          ]
        },
        {
          "name": "Get Distribution Data",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/leads/distribution",
              "host": ["{{baseUrl}}"],
              "path": ["leads", "distribution"]
            }
          },
          "response": [
            {
              "name": "Distribution Data",
              "originalRequest": {
                "method": "GET",
                "header": [
                  {
                    "key": "Authorization",
                    "value": "Bearer {{authToken}}"
                  }
                ],
                "url": {
                  "raw": "{{baseUrl}}/leads/distribution",
                  "host": ["{{baseUrl}}"],
                  "path": ["leads", "distribution"]
                }
              },
              "status": "OK",
              "code": 200,
              "body": "{\n  \"success\": true,\n  \"data\": [\n    {\n      \"agentId\": 1,\n      \"agentName\": \"John Doe\",\n      \"lists\": [\n        {\n          \"firstName\": \"Customer 1\",\n          \"phone\": \"+1234567890\",\n          \"notes\": \"Sample note\",\n          \"assignedAt\": \"2024-01-01T00:00:00.000Z\"\n        }\n      ]\n    }\n  ]\n}"
            }
          ]
        },
        {
          "name": "Get Agent Leads",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/leads/agent/1",
              "host": ["{{baseUrl}}"],
              "path": ["leads", "agent", "1"]
            }
          },
          "response": [
            {
              "name": "Agent Leads",
              "originalRequest": {
                "method": "GET",
                "header": [
                  {
                    "key": "Authorization",
                    "value": "Bearer {{authToken}}"
                  }
                ],
                "url": {
                  "raw": "{{baseUrl}}/leads/agent/1",
                  "host": ["{{baseUrl}}"],
                  "path": ["leads", "agent", "1"]
                }
              },
              "status": "OK",
              "code": 200,
              "body": "{\n  \"success\": true,\n  \"data\": [\n    {\n      \"id\": 1,\n      \"firstName\": \"Customer 1\",\n      \"phone\": \"+1234567890\",\n      \"notes\": \"Sample note\",\n      \"createdAt\": \"2024-01-01T00:00:00.000Z\"\n    }\n  ]\n}"
            }
          ]
        }
      ]
    },
    {
      "name": "Assignments",
      "item": [
        {
          "name": "Get All Assignments",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/assignments",
              "host": ["{{baseUrl}}"],
              "path": ["assignments"]
            }
          },
          "response": [
            {
              "name": "All Assignments",
              "originalRequest": {
                "method": "GET",
                "header": [
                  {
                    "key": "Authorization",
                    "value": "Bearer {{authToken}}"
                  }
                ],
                "url": {
                  "raw": "{{baseUrl}}/assignments",
                  "host": ["{{baseUrl}}"],
                  "path": ["assignments"]
                }
              },
              "status": "OK",
              "code": 200,
              "body": "{\n  \"success\": true,\n  \"data\": [\n    {\n      \"id\": 1,\n      \"agentId\": 1,\n      \"leadId\": 1,\n      \"createdAt\": \"2024-01-01T00:00:00.000Z\",\n      \"agent\": {\n        \"name\": \"John Doe\",\n        \"email\": \"john@example.com\"\n      },\n      \"lead\": {\n        \"firstName\": \"Customer 1\",\n        \"phone\": \"+1234567890\"\n      }\n    }\n  ]\n}"
            }
          ]
        },
        {
          "name": "Get Assignment Stats",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{authToken}}"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/assignments/stats",
              "host": ["{{baseUrl}}"],
              "path": ["assignments", "stats"]
            }
          },
          "response": [
            {
              "name": "Assignment Statistics",
              "originalRequest": {
                "method": "GET",
                "header": [
                  {
                    "key": "Authorization",
                    "value": "Bearer {{authToken}}"
                  }
                ],
                "url": {
                  "raw": "{{baseUrl}}/assignments/stats",
                  "host": ["{{baseUrl}}"],
                  "path": ["assignments", "stats"]
                }
              },
              "status": "OK",
              "code": 200,
              "body": "{\n  \"success\": true,\n  \"data\": {\n    \"totalAssignments\": 25,\n    \"totalAgents\": 5,\n    \"totalLeads\": 25,\n    \"averagePerAgent\": 5,\n    \"distribution\": [\n      {\n        \"agentName\": \"John Doe\",\n        \"count\": 5\n      }\n    ]\n  }\n}"
            }
          ]
        }
      ]
    }
  ],
  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Set base URL if not already set",
          "if (!pm.collectionVariables.get('baseUrl')) {",
          "    pm.collectionVariables.set('baseUrl', 'http://localhost:5001/api');",
          "}"
        ]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [
          "// Global test for all requests",
          "pm.test('Response time is less than 5000ms', () => {",
          "    pm.expect(pm.response.responseTime).to.be.below(5000);",
          "});",
          "",
          "pm.test('Response has success field', () => {",
          "    if (pm.response.code === 200 || pm.response.code === 201) {",
          "        const response = pm.response.json();",
          "        pm.expect(response).to.have.property('success');",
          "    }",
          "});"
        ]
      }
    }
  ]
}